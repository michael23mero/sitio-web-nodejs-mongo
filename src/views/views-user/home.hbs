<div class="row mt-4 py-4">
    <div class="col-md-12 mt-5">
        <h1 class="text-center py-4">Welcome {{ user.username }} <hr></h1>
    </div>
</div>
<div class="row">
    {{#each data}}
    <div class="col-md-3">
        <div class="card p-4 w-100 shadow">
            <h4 class="d-flex justify-content-between align-items-center">
                {{ title }} <a href="/form/{{_id}}"><i class="fas fa-edit"></i></a>
            </h4>
            <img src="{{ url }}" alt="" class="m-2">
            <a href="/post-remove/{{_id}}" class="btn btn-danger btn-block"><i class="fas fa-trash"></i> Remove</a>
        </div>
    </div>
    {{/each}}
</div>