<div class="row mt-4 py-4">
    <div class="col-md-12 mt-5">
        <h1 class="text-center py-4">Welcome {{ user.username }} <hr></h1>
    </div>
</div>
<div class="row">
    <div class="col-md-3">
    {{#each data}}
        <div class="card p-4 w-100 shadow">
            <h4 class="d-flex justify-content-between align-items-center">
                {{ title }} <a href="/post-update/{{_id}}"><i class="fas fa-edit"></i></a>
            </h4>
            <img src="{{ url }}" alt="nombre-de-imagen" class="m-2">
            <p class="content-justify">{{ descp }}</p>
            <a href="/post-delete/{{_id}}" class="btn btn-danger btn-block"><i class="fas fa-trash"></i> Remove</a>
        </div>
    {{/each}}
    </div>

    <div class="col-md-7">
    {{#each datos}}
        <div class="m-2 card p-4">
            <h4>{{title}}</h4>
            <img src="{{ url }}" alt="nombre-de-imagen" class="m-2">
            <p class="content-justify">{{ descp }}</p>
        </div>
    {{/each}}
    </div>

    <div class="col-md-2">
        <h4 class="text-center">Setting</h4>
    </div>
</div>